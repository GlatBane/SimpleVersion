{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../../src"
        }
      ],
      "dest": "obj/_api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**.{md,yml}" ],
        "src": "obj/_api",
        "dest": "api"
      },
      {
        "files": [ "**/*.{md,yml}" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": "apispec/*.md",
    "dest": "obj/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "statictoc", "./theme" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appLogo": "images/logo.png"
    }
  }
}
