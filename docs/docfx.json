{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/bin/**/Simpleversion*" ],
          "exclude": [ "**/obj/**", "**/*.{xml,pdb,config}" ],
          "src": "../src"
        }
      ],
      "dest": "site/api",
      "properties": {
        "TargetFramework": "net461"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**.{md,yml}" ],
        "src": "site/_api"
      },
      {
        "files": [ "**/*.{md,yml}" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "overwrite": "apispec/*.md",
    "dest": "site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "statictoc", "./theme" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appLogo": "images/logo.png"
    }
  }
}
